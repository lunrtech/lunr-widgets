{
  "compilerOptions": {
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "rootDir": "./src",
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "skipLibCheck": true,
    "types": []

  },
  "angularCompilerOptions": {
    "genDir": "compiled"
  }
}